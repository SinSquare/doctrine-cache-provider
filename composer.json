{
    "name": "sinsquare/silex-doctrine-cache-provider",
    "description": "Doctrine cache provider for Silex framework",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "~5.6|~7.0",
        "doctrine/cache": "*",
        "pimple/pimple": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": { "SinSquare\\Cache\\": "src/Sins/Cache" }
    },
    "autoload-dev": {
        "psr-4": { "SinSquare\\Tests\\": "tests/Sins/Tests" }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
