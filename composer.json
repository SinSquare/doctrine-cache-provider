{
    "name": "sinsquare/doctrine-cache-provider",
    "description": "Doctrine cache provider for Silex framework",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "~5.6|~7.0",
        "doctrine/cache": "*",
        "silex/silex": "*"
    },
    "autoload": {
        "psr-4": { "SinSquare\\Cache\\": "src/Sins/Cache" }
    },
    "autoload-dev": {
        "psr-4": { "SinSquare\\Tests\\": "tests/Sins/Tests" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
